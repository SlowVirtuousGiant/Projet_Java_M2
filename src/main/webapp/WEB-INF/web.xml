    <servlet-class>fr.dauphine.sj.monrocqxu.appMedecin.web.Ajout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Ajout</servlet-name>
    <url-pattern>/ajout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Espace</servlet-name>
