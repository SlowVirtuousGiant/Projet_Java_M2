    <servlet-name>Modification</servlet-name>
    <servlet-class>fr.dauphine.sj.monrocqxu.appMedecin.web.Modification</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Modification</servlet-name>
    <url-pattern>/modification</url-pattern>
  </servlet-mapping>
